    <!-- Separators -->
    <Separators>
        <DecimalSeparator>.</DecimalSeparator>
        <ThousandsSeparator>,</ThousandsSeparator>
        <ListSeparator>,</ListSeparator>
    </Separators>
    <!-- Quote Characters -->
    <Quotes>
        <Quote type="opening" use="standard" scope="primary">“</Quote>
        <Quote type="closing" use="standard" scope="primary">”</Quote>
        <Quote type="opening" use="standard" scope="secondary">‘</Quote>
        <Quote type="closing" use="standard" scope="secondary">’</Quote>
        <Quote type="alternate" use="standard" scope="primary">‘</Quote>
        <Quote type="alternate" use="standard" scope="primary">’</Quote>
        <Quote type="alternate" use="standard" scope="secondary">“</Quote>
        <Quote type="alternate" use="standard" scope="secondary">”</Quote>
        <Spacing>1.5</Spacing>
    </Quotes>
    <!-- Date and Time Formatting -->
    <!-- format types are short, standard, and long -->
    <DateFormats>
        <DateFormat type="short">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[M]/[D]/[Y0001]</Picture>
        </DateFormat>
        <DateFormat type="standard">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[MNn,*-4] [Y0001]</Picture>
            <Picture components="3">[MNn,*-4] [D], [Y0001]</Picture>
        </DateFormat>
        <DateFormat type="long">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[MNn] [Y0001]</Picture>
            <Picture components="3">[MNn] [D], [Y0001]</Picture>
        </DateFormat>
    </DateFormats>
    <TimeFormats>
        <TimeFormat type="standard">
            <Picture components="1">[H01]</Picture>
            <Picture components="2">[H01]:[m01]</Picture>
            <Picture components="3">[H01]:[m01]:[s01]</Picture>
            <Picture components="4">[H01]:[m01]:[s01],[f,3-3]</Picture>
        </TimeFormat>
    </TimeFormats>
    <DateTimeFormats>
        <DateTimeFormat type="short">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[M]/[D]/[Y0001]</Picture>
            <Picture components="4">[M]/[D]/[Y0001] [H01]</Picture>
            <Picture components="5">[M]/[D]/[Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[M]/[D]/[Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[M]/[D]/[Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
        <DateTimeFormat type="standard">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[MNn,*-4] [Y0001]</Picture>
            <Picture components="3">[MNn,*-4] [D], [Y0001]</Picture>
            <Picture components="4">[MNn,*-4] [D], [Y0001] [H01]</Picture>
            <Picture components="5">[MNn,*-4] [D], [Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[MNn,*-4] [D], [Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[MNn,*-4] [D], [Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
        <DateTimeFormat type="long">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[MNn] [Y0001]</Picture>
            <Picture components="3">[MNn] [D], [Y0001]</Picture>
            <Picture components="4">[MNn] [D], [Y0001] [H01]</Picture>
            <Picture components="5">[MNn] [D], [Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[MNn] [D], [Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[MNn] [D], [Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
    </DateTimeFormats>
