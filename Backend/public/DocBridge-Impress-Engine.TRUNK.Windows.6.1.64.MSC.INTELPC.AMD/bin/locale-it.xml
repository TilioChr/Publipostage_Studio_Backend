<?xml version="1.0" encoding="UTF-8"?>

<Language id="it" defCountry="IT" xmlns="http://www.compart.com/ns/locale">
    <!-- Separators -->
    <Separators>
        <DecimalSeparator>,</DecimalSeparator>
        <ThousandsSeparator>.</ThousandsSeparator>
        <ListSeparator>;</ListSeparator>
    </Separators>
    <!-- Quote Characters -->
    <Quotes>
        <Quote type="opening" use="standard" scope="primary">«</Quote>
        <Quote type="closing" use="standard" scope="primary">»</Quote>
        <Quote type="opening" use="standard" scope="secondary">‹</Quote>
        <Quote type="closing" use="standard" scope="secondary">›</Quote>
        <Quote type="alternate" use="standard" scope="primary">“</Quote>
        <Quote type="alternate" use="standard" scope="primary">”</Quote>
        <Quote type="alternate" use="standard" scope="secondary">‘</Quote>
        <Quote type="alternate" use="standard" scope="secondary">’</Quote>
        <Spacing>-4</Spacing>
    </Quotes>
    <!-- Date and Time Formatting -->
    <!-- format types are short, standard, and long -->
    <!--  -->
    <DateFormats>
        <DateFormat type="short">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[D]/[M]/[Y0001]</Picture>
        </DateFormat>
        <DateFormat type="standard">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[D]/[M]/[Y0001]</Picture>
        </DateFormat>
        <DateFormat type="long">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[Mn] [Y0001]</Picture>
            <Picture components="3">[D] [Mn] [Y0001]</Picture>
        </DateFormat>
    </DateFormats>
    <TimeFormats>
        <TimeFormat type="short">
            <Picture components="1">[H01]</Picture>
            <Picture components="2">[H01]:[m01]</Picture>
            <Picture components="3">[H01]:[m01]:[s01]</Picture>
            <Picture components="4">[H01]:[m01]:[s01],[f,3-3]</Picture>
        </TimeFormat>
        <TimeFormat type="standard">
            <Picture components="1">[H01]</Picture>
            <Picture components="2">[H01]:[m01]</Picture>
            <Picture components="3">[H01]:[m01]:[s01]</Picture>
            <Picture components="4">[H01]:[m01]:[s01],[f,3-3]</Picture>
        </TimeFormat>
    </TimeFormats>
    <DateTimeFormats>
        <DateTimeFormat type="short">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[D]/[M]/[Y0001]</Picture>
            <Picture components="4">[D]/[M]/[Y0001] [H01]</Picture>
            <Picture components="5">[D]/[M]/[Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[D]/[M]/[Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[D]/[M]/[Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
        <DateTimeFormat type="standard">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[M]/[Y0001]</Picture>
            <Picture components="3">[D]/[M]/[Y0001]</Picture>
            <Picture components="4">[D]/[M]/[Y0001] [H01]</Picture>
            <Picture components="5">[D]/[M]/[Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[D]/[M]/[Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[D]/[M]/[Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
        <DateTimeFormat type="long">
            <Picture components="1">[Y0001]</Picture>
            <Picture components="2">[Mn] [Y0001]</Picture>
            <Picture components="3">[D] [Mn] [Y0001]</Picture>
            <Picture components="4">[D] [Mn] [Y0001] [H01]</Picture>
            <Picture components="5">[D] [Mn] [Y0001] [H01]:[m01]</Picture>
            <Picture components="6">[D] [Mn] [Y0001] [H01]:[m01]:[s01]</Picture>
            <Picture components="7">[D] [Mn] [Y0001] [H01]:[m01]:[s01],[f,3-3]</Picture>
        </DateTimeFormat>
    </DateTimeFormats>
