    <!-- Numeral Formatting -->
    <Numerals defUntil="1" onesFirst="false" tensSeparator="" hundredsSeparator=""
        thousandsSeparator="" ordinalSuffix="" ordinalRemove="a">
        <OrdinalSuffixes>
            <!-- Die Ordnungszahlen im Italienischen verhalten sich wie Adjektive und richten sich somit in Genus und Numerus nach dem Substantiv, auf das sie sich beziehen. 
                Anders als im Deutschen, verwendet man im Italienischen beim Datum keine Ordnungszahlen, sondern die normalen Grundzahlen.
        -->
            <OrdinalSuffix module="1" reminder="0">°</OrdinalSuffix>
        </OrdinalSuffixes>
        <Specials>
            <Numbers>
                <Number value="0" cardinal="zero"/>
                <Number value="1" cardinal="uno" ordinal="primo"/>
                <!-- un, una -->
                <Number value="2" cardinal="due" ordinal="secondo"/>
                <Number value="3" cardinal="tre" ordinal="terzo"/>
                <Number value="4" cardinal="quattro" ordinal="quarto"/>
                <Number value="5" cardinal="cinque" ordinal="quinto"/>
                <Number value="6" cardinal="sei" ordinal="sesto"/>
                <Number value="7" cardinal="sette" ordinal="settimo"/>
                <Number value="8" cardinal="otto" ordinal="ottavo"/>
                <Number value="9" cardinal="nove" ordinal="nono"/>
                <Number value="10" cardinal="dieci" ordinal="decimo"/>
                <Number value="11" cardinal="undici" ordinal="undicesimo"/>
                <Number value="12" cardinal="dodici" ordinal="dodicesimo"/>
                <Number value="13" cardinal="tredici" ordinal="tredicesimo"/>
                <Number value="14" cardinal="quattordici" ordinal="quattordicesimo"/>
                <Number value="15" cardinal="quindici" ordinal="quindicesimo"/>
                <Number value="16" cardinal="sedici" ordinal="sedicesimo"/>
                <Number value="17" cardinal="diciassette" ordinal="diciassettesimo"/>
                <Number value="18" cardinal="diciotto" ordinal="diciottesimo"/>
                <Number value="19" cardinal="diciannove" ordinal="diciannovesimo"/>
                <Number value="20" cardinal="venti" ordinal="ventesimo"/>
                <Number value="21" cardinal="ventuno"/>
                <Number value="22" cardinal="ventidue"/>
                <Number value="23" cardinal="ventitre"/>
                <Number value="24" cardinal="ventiquattro"/>
                <Number value="25" cardinal="venticinque"/>
                <Number value="26" cardinal="ventisei"/>
                <Number value="27" cardinal="ventisette"/>
                <Number value="28" cardinal="ventotto"/>
                <Number value="29" cardinal="ventinove"/>
                <Number value="30" cardinal="trenta" ordinal="trentesimo"/>
                <!-- trentuno, trentadue, ... -->
                <Number value="31" cardinal="trentuno"/>
                <Number value="40" cardinal="quaranta" ordinal="quarantesimo"/>
                <Number value="50" cardinal="cinquanta" ordinal="cinquantesimo"/>
                <Number value="60" cardinal="sessanta" ordinal="sessantesimo"/>
                <Number value="70" cardinal="settanta" ordinal="settantesimo"/>
                <Number value="80" cardinal="ottanta" ordinal="ottantesimo"/>
                <Number value="90" cardinal="novanta" ordinal="novantesimo"/>
                <Number value="100" cardinal="cento" plural="cento" singular="cent "
                    ordinal="centesimo"/>
                <Number value="500" cardinal="cinquecento "/>
                <Number value="1000" plural="mila" singular="mille" ordinal="milésimo"/>
                <Number value="1000000" plural="milioni" singular="milione" ordinal="millonésimo"/>
                <Number value="1000000000000" plural="bilioni" singular="bilione"/>
                <Number value="1000000000000000000" plural="trilioni" singular="trilione"/>
            </Numbers>
        </Specials>
    </Numerals>
