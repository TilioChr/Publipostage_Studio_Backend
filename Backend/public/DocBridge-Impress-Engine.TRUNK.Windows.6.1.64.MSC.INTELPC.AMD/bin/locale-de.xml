                <Number value="16" cardinal="sechzehn" ordinal="sechzehnte"/>
                <Number value="17" cardinal="siebzehn" ordinal="siebzehnte"/>
                <Number value="18" cardinal="achtzehn" ordinal="achtzehnte"/>
                <Number value="19" cardinal="neunzehn" ordinal="neunzehnte"/>
                <Number value="20" cardinal="zwanzig"/>
                <Number value="30" cardinal="dreißig"/>
                <Number value="40" cardinal="vierzig"/>
                <Number value="50" cardinal="fünfzig"/>
                <Number value="60" cardinal="sechzig"/>
                <Number value="70" cardinal="siebzig"/>
                <Number value="80" cardinal="achtzig"/>
                <Number value="90" cardinal="neunzig"/>
                <Number value="100" plural="hundert"/>
                <Number value="1000" singular="eintausend" plural="tausend"/>
                <Number value="1000000" singular="eine Million " plural=" Millionen "/>
                <Number value="1000000000" singular="eine Milliarde " plural=" Milliarden "/>
                <Number value="1000000000000" singular="eine Billion " plural=" Billionen "/>
                <Number value="1000000000000000" singular="eine Billiarde " plural=" Billiarden "/>
                <Number value="1000000000000000000" singular="eine Trillion " plural=" Trillionen "/>
