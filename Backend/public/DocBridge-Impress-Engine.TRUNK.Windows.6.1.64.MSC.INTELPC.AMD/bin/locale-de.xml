                    plural=" Trilliarden "/>
            </Numbers>
        </Specials>
    </Numerals>
    <!-- Hypenation Rules -->
    <Hyphenations pushCharacterCount="2" remainCharacterCount="2" hyphenationCharacter="-">
        <Entries hyphenChar="-">
            <Entry>Ab-fra-ge</Entry>
            <Entry>Abschnitt</Entry>
            <Entry>Än-dern</Entry>
            <Entry>Än-de-rungs-num-mer</Entry>
            <Entry>an-schau-en</Entry>
            <Entry>An-wei-sung</Entry>
            <Entry>an-wen-den</Entry>
            <Entry>An-wen-der</Entry>
            <Entry>Auf-klapp-liste</Entry>
            <Entry lineOverflow="false">DocBridge</Entry>
            <Entry overflowHyphenChar="*">kling*onisch</Entry>
            <Entry>bla<Hyphenate/>bla</Entry>
            <Entry>schic<Hyphenate hyphenationRule="ruleCKKK"/>ken</Entry>
            <Entry>Schif<Hyphenate hyphenationRule="ruleDoubleConsonant"/>fahrt</Entry>
        </Entries>
        <Patterns src="hyph-de-1996.tex"/>
    </Hyphenations>
