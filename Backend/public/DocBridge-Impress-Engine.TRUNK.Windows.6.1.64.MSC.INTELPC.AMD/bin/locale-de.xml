    <!-- Numeral Formatting -->
    <Numerals defUntil="12" onesFirst="true" tensSeparator="und" hundredsSeparator=""
        thousandsSeparator="" ordinalSuffix="ste" ordinalRemove="">
        <OrdinalSuffixes>
            <OrdinalSuffix module="1" reminder="0">.</OrdinalSuffix>
        </OrdinalSuffixes>
        <Specials>
            <Numbers>
                <Number value="0" cardinal="null" ordinal="nullte"/>
                <Number value="1" cardinal="ein" lastCardinal="eins" ordinal="erste"/>
