    <Holidays id="all">
        <Holiday key="intlWomensDay" type="O" format="absolute" day="8" month="3" since="1977"
            until="indefinitely" every="1"/>
        <Holiday key="universalChildrensDay" type="O" format="absolute" day="20" month="11"
            since="1954" until="indefinitely" every="1"/>
    </Holidays>
    <Holidays id="christian" inheritsFrom="all">
        <Holiday key="epiphany" type="C" format="absolute" day="6" month="1" since="500"
            until="indefinitely" every="1"/>
        <Holiday key="palmSunday" type="C" format="eastern" day="-7" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="goodFriday" type="C" format="eastern" day="-2" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="easterSunday" type="C" format="eastern" day="0" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="easterMonday" type="C" format="eastern" day="1" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="ascensionOfJesus" type="C" format="eastern" day="39" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="whitSunday" type="C" format="eastern" day="49" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="whitMonday" type="C" format="eastern" day="50" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="corpusChristi" type="C" format="eastern" day="60" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="ashWesdnesday" type="C" format="eastern" day="-46" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="maundyThursday" type="C" format="eastern" day="-3" since="325"
            until="indefinitely" every="1"/>
        <Holiday key="stValentinesDay" type="C" format="absolute" day="14" month="2" since="1950"
            until="indefinitely" every="1"/>
        <Holiday key="walpurgisNight" type="C" format="absolute" day="30" month="4" since="1700"
            until="indefinitely" every="1"/>
        <Holiday key="assumptionOfMary" type="C" format="absolute" day="15" month="8" since="1300"
            until="indefinitely" every="1"/>
        <Holiday key="reformationDay" type="C" format="absolute" day="31" month="10" since="1617"
            until="indefinitely" every="1"/>
        <Holiday key="allSaintsDay" type="C" format="absolute" day="1" month="11" since="610"
            until="indefinitely" every="1"/>
        <Holiday key="allSoulsDay" type="C" format="absolute" day="2" month="11" since="610"
            until="indefinitely" every="1"/>
        <Holiday key="advent1" type="C" format="relative" day="27" month="11" dayOW="sun"
            since="600" until="indefinitely" every="1"/>
        <Holiday key="stMartinsDay" type="C" format="absolute" day="11" month="11" since="397"
            until="indefinitely" every="1"/>
        <Holiday key="advent2" type="C" format="relative" day="4" month="12" dayOW="sun" since="600"
            until="indefinitely" every="1"/>
        <Holiday key="stNicholasDay" type="C" format="absolute" day="6" month="12" since="365"
            until="indefinitely" every="1"/>
        <Holiday key="advent3" type="C" format="relative" day="11" month="12" dayOW="sun"
            since="600" until="indefinitely" every="1"/>
        <Holiday key="advent4" type="C" format="relative" day="18" month="12" dayOW="sun"
            since="600" until="indefinitely" every="1"/>
        <Holiday key="christmasEve" type="C" format="absolute" day="24" month="12" since="354"
            until="indefinitely" every="1"/>
        <Holiday key="christmasDay" type="C" format="absolute" day="25" month="12" since="354"
            until="indefinitely" every="1"/>
        <Holiday key="boxingDay" type="C" format="absolute" day="26" month="12" since="354"
            until="indefinitely" every="1"/>
