                <Number value="12" cardinal="doce" ordinal="duodécimo"/>
                <Number value="13" cardinal="trece" ordinal="decimotercero"/>
                <Number value="14" cardinal="catorce" ordinal="decimocuarto"/>
                <Number value="15" cardinal="quince" ordinal="decimoquinto"/>
