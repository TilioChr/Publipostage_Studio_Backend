   <globals>
      <!--Specifies whether the PA is to be optimized. Valid values are: ALWAYS:
                Always optimize the PA. NEVER: Never optimize the PA. -->

      <optimizepa value="auto"/>

      <colorindexlist>
         <colorindex index="0" color="#00FFFFFF"/>

         <!--BLACK-->

         <colorindex index="1" color="#00000000"/>

         <!--RED-->

         <colorindex index="2" color="#00FF0000"/>

         <!--GREEN-->

         <colorindex index="3" color="#0000FF00"/>

         <!--YELLOW-->

         <colorindex index="4" color="#00FFFF00"/>

         <!--BLUE-->

