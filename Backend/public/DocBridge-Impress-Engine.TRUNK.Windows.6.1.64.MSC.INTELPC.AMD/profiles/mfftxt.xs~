                                 <xs:annotation>
                                    <xs:documentation xml:lang="EN">Columns with special meaning</xs:documentation>
                                 </xs:annotation>
                                 <xs:complexType>
                                    <xs:attribute name="count" default="0" type="xs:integer"/>
