                                       <xs:simpleType>
                                          <xs:restriction base="xs:string">
                                             <xs:enumeration value="TrueType"/>
                                             <xs:enumeration value="Type1"/>
                                          </xs:restriction>
                                       </xs:simpleType>
                                    </xs:attribute>
                                 </xs:complexType>
                              </xs:element>
                           </xs:sequence>
                           <xs:attribute name="family" default="Courier New" type="xs:string"/>
