                           <xs:attribute name="value" default="TLE, NOP, IMM" type="xs:string">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US">Determines the order in which TLEs, NOPs and IMM are
                written.</xs:documentation>
                                 <xs:documentation xml:lang="de-DE">Bestimmt, in welcher Reihenfolge TLEs, NOPs und IMM
                geschrieben werden.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="tlewritemode" default="strict">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US">strict (default): Writing of TLEs is restricted to the places
                allowed by the AFP specification. relaxed: Writing of TLEs is allowed at all
                positions.</xs:documentation>
                                 <xs:documentation xml:lang="de-DE">strict (default): Das Schreiben von TLEs ist auf die per
                AFP-Spezifikation erlaubten Positionen beschränkt. relaxed: Das Schreiben von TLEs
                ist an allen Positionen erlaubt.</xs:documentation>
                              </xs:annotation>
                              <xs:simpleType>
                                 <xs:restriction base="xs:string">
                                    <xs:enumeration value="strict"/>
                                    <xs:enumeration value="relaxed"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:attribute>
