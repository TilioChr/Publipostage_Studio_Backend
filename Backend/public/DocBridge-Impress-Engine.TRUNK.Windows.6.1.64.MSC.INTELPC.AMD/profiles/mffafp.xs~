                           <xs:attribute name="value" default="true" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US">true: Read archive fields. false: Ignore archive
                fields.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="onceperpagegroup" default="false" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US">true: Page group TLEs are visible only before the first page
                of the page group. false (default): Page group TLEs are visible on all input pages
                of the page group.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="unify" default="true" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US">true: Provide TLEs in a manner where all output filters using
                archivefields can find them. (Default) false: Provide TLEs in new extend mode.
            </xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                        </xs:complexType>
                     </xs:element>
