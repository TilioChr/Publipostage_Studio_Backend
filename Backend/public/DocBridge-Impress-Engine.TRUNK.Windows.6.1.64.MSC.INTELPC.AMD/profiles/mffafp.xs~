                              <xs:annotation>
                                 <xs:documentation xml:lang="en-US"> true (default): Write encoding triplet. false: Do not write
                encoding triplet.</xs:documentation>
                                 <xs:documentation xml:lang="de-DE">true (default): Encoding Triplet wird geschrieben. false:
                Encoding Triplet wird nicht geschrieben.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                        </xs:complexType>
