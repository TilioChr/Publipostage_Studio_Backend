                           <xs:attribute name="generatepopups" default="false" type="xs:boolean"/>
                           <xs:attribute name="generatefields" default="false" type="xs:boolean"/>
                           <xs:attribute name="printall" default="false" type="xs:boolean"/>
                           <xs:attribute name="generatearchivefields" default="true" type="xs:boolean"/>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="render-page" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                           <xs:documentation xml:lang="en-US">Specifies whether pages of the input document are rendered
                while being read.</xs:documentation>
                           <xs:documentation xml:lang="de-DE">Definiert, ob die Seiten der Eingangsdokumente beim Lesen
                gerendert werden sollen.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="trigger-item" minOccurs="0" maxOccurs="unbounded">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en-US">Specifies certain page objects if the rendering is to be
