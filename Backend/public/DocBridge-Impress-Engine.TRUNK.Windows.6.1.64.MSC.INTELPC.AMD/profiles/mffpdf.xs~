                                             <xs:enumeration value="Widget"/>
                                             <xs:enumeration value="Polygon"/>
                                             <xs:enumeration value="PolyLine"/>
                                             <xs:enumeration value="Screen"/>
                                          </xs:restriction>
                                       </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="read" default="true" type="xs:string"/>
