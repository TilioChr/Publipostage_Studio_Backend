                           <xs:attribute name="resolution" default="300" type="xs:integer"/>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="textclipping" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                           <xs:documentation xml:lang="en-US">Specifies how clipping information is handled. Depending on
                the format, this can facilitate the conversion into the output format.</xs:documentation>
                           <xs:documentation xml:lang="de-DE">Definiert, wie mit Clipping-Informationen umgegangen werden
                soll. Je nach Format, kann dies die Konvertierung in das Ausgabeformat deutlich
                erleichtern.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:attribute name="value" default="Default">
                              <xs:simpleType>
                                 <xs:restriction base="xs:string">
                                    <xs:enumeration value="Default"/>
                                    <xs:enumeration value="ClipToPage"/>
                                    <xs:enumeration value="Cut-Clipped-Text"/>
                                    <xs:enumeration value="Clipping-Paths"/>
                                 </xs:restriction>
