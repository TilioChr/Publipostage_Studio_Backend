   <!--Settings for input.-->

   <input>
      <!-- please remember tray id is printer dependent, you must read the printer
                documentation to get it. -->

      <traylist>
         <inputtrays>
            <inputtray deviceid="0" name="Default"/>
            <inputtray deviceid="1" name="AutoSelect"/>
            <inputtray deviceid="2" name="ManualFeed"/>
            <inputtray deviceid="3" name="MultiPurpose"/>
            <inputtray deviceid="4" name="UpperCassette"/>
            <inputtray deviceid="5" name="LowerCassette"/>
            <inputtray deviceid="6" name="Envelope"/>
            <inputtray deviceid="7" name="ThirdCassette"/>
         </inputtrays>

         <outputtrays>
            <outputtray deviceid="0" name="Default"/>
            <outputtray deviceid="1" name="FaceDown"/>
            <outputtray deviceid="2" name="FaceUp"/>
            <outputtray deviceid="3" name="JobOffset"/>
         </outputtrays>
      </traylist>

      <!-- the following table specify the value of the default media size in pxl
                _________________________________________ | page size | value |
                _________________________________________ | Letter | 0 |
                _________________________________________ | Legal | 1 |
                _________________________________________ | A4 | 2 |
                _________________________________________ | Executive | 3 |
                _________________________________________ | Ledger | 4 |
                _________________________________________ | A3 | 5 |
                _________________________________________ | COM 10 Envelope | 6 |
                _________________________________________ | Monarch Envelope | 7 |
                _________________________________________ | C5 Envelope | 8 |
                _________________________________________ | DL Envelope | 9 |
                _________________________________________ | JIS B4 Paper | 10 |
                _________________________________________ | JIS B5 Paper | 11 |
                _________________________________________ | B5 Envelope | 12 |
                __________________________________________ | B5 Paper | 13 |
                __________________________________________ | JPostcard | 14 |
                _________________________________________ | JDouble Postcard | 15 |
                __________________________________________ | A5 | 16 |
                _________________________________________ | A6 | 17 |
                _________________________________________ | JIS B6 Paper | 18 |
                _________________________________________ | JIS 8K | 19 |
                __________________________________________ | JIS 16K | 20 |
                __________________________________________ | JIS Exec | 21 |
                __________________________________________ The default page size ist A4. You can see
                all of the default setting in your printer menu or documentation. -->

      <defaultsetting>
         <pagesize value="2"/>
         <orientation value="portrait"/>
         <duplexcontrol value="simplex"/>
      </defaultsetting>

      <!-- The name is media size name in pxl file. It must be unique. Orientation
                must be in portrait Paper size can be specified by "format" and "orientation" or
                "width" and "height"-->

      <pagedefinition name="LETTER 8.5x11">
         <papersize format="Letter" orientation="portrait"/>
      </pagedefinition>
   </input>

   <!--Settings for output.-->

   <output>
      <!-- PXL version for output. The common value is 1.1, 2.0, 2.1 and 3.0. Default
                value is 2.1. The measure unit in BeginSession and SetPageScale must be the same in
                v2.0 -->

      <version value="2.1"/>

      <!-- The 'value' specifies the available resolutions in dpi and
                can contain a list of permissible values as a comma-separated list
                ('value="600,1200"'). If a value list is specified, the specified resolution from
                the list will be used. If the respective value is not available, then the highest
                value will be used. If no values are specified, one of the internally defined
                default values 1200dpi or 1440dpi will be used. -->

      <measurementunits value="0"/>

      <!--Hex value for PXL error reporting level for output. Possible values are
                0x00 eNoReporting version 1.1 0x01 eBackChannel(BackCh) version 1.1 0x02
                eErrorPage(ErrPage) version 1.1 Default 0x03 eBackChAndErrPage version 1.1 0x04
                eNWBackChannel version 2.0 0x05 eNWErrorPage version 2.0 0x06 eNWBackChAndErrPage
                version 2.0-->

      <ErrorReporting value="0x02"/>

      <!-- please remember tray id is printer dependent, you must read the printer
                documentation to get it. -->

      <traylist>
         <inputtrays>
            <inputtray deviceid="0" name="Default"/>
            <inputtray deviceid="1" name="AutoSelect"/>
            <inputtray deviceid="2" name="ManualFeed"/>
            <inputtray deviceid="3" name="MultiPurpose"/>
            <inputtray deviceid="4" name="UpperCassette"/>
            <inputtray deviceid="5" name="LowerCassette"/>
            <inputtray deviceid="6" name="Envelope"/>
            <inputtray deviceid="7" name="ThirdCassette"/>
         </inputtrays>

         <outputtrays>
            <outputtray deviceid="0" name="Default"/>
            <outputtray deviceid="1" name="FaceDown"/>
            <outputtray deviceid="2" name="FaceUp"/>
            <outputtray deviceid="3" name="JobOffset"/>
         </outputtrays>
      </traylist>

      <!-- resolution to use for generated raster fonts. If the input font have
                already resolution and output filter can support it, it will be used, otherwise the
                specified value here will be used. default 0: let the application make decision. For
                PXL Font: only 300, 600 and 1200 dpi is allowed. -->

      <fontresolution value="0"/>

      <!--Specifies the quality of written JPEG images as percentage. Valid values
                range from 0 to 100%. The default setting is 75%.-->

      <jpegquality value="75"/>
   </output>

   <resourcelist>
      <fileslist>
         <!--Several directories can be specified for the search for external resources.
                The "path" is mandatory. "Type", "extension" and "resolution" are optional. However,
                the more detailed the settings are, the more specific the search will be. Valid
                values for "type" are: "overlay", "pageSegment", "codedFont", "codePage", "charSet",
                "formDef", "resourceLib", "pageDef", "object", "icc", "documentIndex". If the type
                is not defined, the path will be used for all types. The "extension" can be emtpy,
                <extension> or a wildcard. If the "resolution" is not specified, resources
                with any resolution will be searched in the path. -->
         <!--<files path="." type="TrueType" extension="ttf"/>-->
      </fileslist>
   </resourcelist>
</mffpxl>
