   <!-- In pxl the devname is unique, it must be specified to match the font name
                in pxl file Font specified by "devname", "family", "weight" and "style", optional:
                style can be "UPRIGHT" or "ITALIC" weight can be "ULTRALIGHT", "EXTRALIGHT",
                "LIGHT", "SEMILIGHT", "MEDIUM", "SEMIBOLOD", "BOLD", "EXTRABOLD", "ULTRABOLD"
                "typeface" is only for PCL input. The pcl select font with font characteristic
                including typeface. If you want to add a new font, just specify the font devname,
                family name, weight, style and typeface in font list. -->

   <fontlist>
      <font family="Letter Gothic" typeface="4102">
         <face devname="LetterGothic" weight="Medium" style="Upright" fontfile="letr45w" fontfiletype="TrueType"/>
         <face devname="LetterGothic  It" weight="Medium" style="Italic" fontfile="letr46w" fontfiletype="TrueType"/>
         <face devname="LetterGothic  Bd" weight="Bold" style="Upright" fontfile="letr65w" fontfiletype="TrueType"/>
      </font>

