   <!--The list of TCP/IP connections.-->

   <tcpipconnections>
      <!--The declaration of a TCP/IP connection.-->
      <!--Attribute: name @comment: The name of a TCP/IP connection. The default value is
                "NAME_EXAMPLE_VALUE".-->
      <!--Attribute: host @comment: The IP address of the IPDS printer. The default value is
                "127.0.0.1".-->
      <!--Attribute: port @comment: The port number of the IPDS printer. The default value is
                "5001".-->

      <tcpipconnection name="NAME_EXAMPLE_VALUE" host="127.0.0.1" port="5001"/>
      <tcpipconnection name="TCPIP_InfoPrint_1585" host="172.20.17.26" port="5001"/>
      <tcpipconnection name="TCPIP_InfoPrint_1985" host="172.20.17.222" port="5001"/>
      <tcpipconnection name="TCPIP_InfoPrint_Color_1834" host="172.20.17.221" port="5001"/>
      <tcpipconnection name="TCPIP_InfoPrint_70" host="172.20.17.22" port="5001"/>
      <tcpipconnection name="TCPIP_Loopback" host="127.0.0.1" port="5001"/>
      <tcpipconnection name="TCPIP_Receiver" host="127.0.0.1" port="5001"/>
   </tcpipconnections>

   <!--Contains a list of locations for resource files.-->

   <resourcelist>
      <!--Contains a list of file paths.-->

      <fileslist>
         <files path="c:\WINDOWS\Fonts" type="truetype" extension="ttf"/>
         <files path="\resource" type="icc" extension="icc"/>
         <files path="\resource" type="icc" extension="icm"/>
      </fileslist>
   </resourcelist>
</mffips>
